#include <unistd.h>
#include <sys/types.h>
#include <stdlib.h>
#include <string.h>
#include <stdio.h>

int main (int ac, char **av) {
	char buffer[64];
	char *cmd;

	int a = atoi(av[1]);
	if (a == 0x1a7) {
		cmd = strdup("/bin/sh");
		int proc_gid = getegid();
		int proc_uid = geteuid();

		setresgid(proc_gid, proc_gid, proc_gid);
		setresuid(proc_uid, proc_uid, proc_uid);
		execv(cmd, NULL);
	} else {
		fwrite("No !\n", 0x1, 0x5, stderr);
	}
	return (0);
}
